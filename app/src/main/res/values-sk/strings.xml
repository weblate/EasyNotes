    <string name="terms_liability_title">6. Obmedzenie zodpovednosti</string>
    <string name="terms_termination_title">7. Ukončenie</string>
    <string name="terms_liability_body">Nenesieme zodpovednosť za žiadne škody vyplývajúce z používania aplikácie Easy Notes vrátane nepriamych, náhodných alebo následných škôd.</string>
